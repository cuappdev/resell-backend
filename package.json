      "math.js": "^1.1.46",
      "multer": "1.4.5-lts.1",
      "node-fetch": "^2.6.1",
      "pg": "^8.7.1",
      "pgvector": "^0.2.0",
      "pug": "^3.0.2",
      "reflect-metadata": "^0.1.13",
      "routing-controllers": "0.10.2",
      "string-similarity": "^4.0.4",
      "swagger-ui-express": "^5.0.1",
      "ts-node": "^10.5.0",
      "typedi": "^0.10.0",
      "typeorm": "^0.2.45",
      "typeorm-naming-strategies": "^4.1.0",
